# venv-ul se gaseste in directorul parinte: .venv
# source ../.venv/bin/activate
. .venv/bin/activate

# daca comanda de mai sus esueaza,se creaza .venv si se instaleaza dependintele
# venv-ul va ramane activat in cazul:
if [ $? -eq 0 ]
then
    echo "SUCCESS: venv was activated."
else
    echo "FAIL: cannot activate venv"
    . ./activeaza_venv_jenkins
fi
